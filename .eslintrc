{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "import/first": "error",
    "react/prop-types": 0,
    "prettier/prettier": "error",
    "linebreak-style": [
      "error",
      "unix"
    ],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}