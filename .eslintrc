{
  "extends": "airbnb",
  "rules": {
    "valid-jsdoc": 2,
    "no-var": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "func-names": 0,
    "new-cap": 0,
    "no-undef": 0
  },
  "plugins": [
    "react"
  ],
  "overrides": [
    {
      "files": "client/src/*.jsx",
      "rules": {
        "import/extensions": "off",
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "no-console": "off"
      }
    }
  ]
}