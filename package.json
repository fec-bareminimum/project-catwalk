{
  "name": "project-catwalk",
  "version": "1.0.0",
  "description": "MPV online-retail frontend client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fec-bareminimum/project-catwalk.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fec-bareminimum/project-catwalk/issues"
  },
  "homepage": "https://github.com/fec-bareminimum/project-catwalk#readme",
  "main": "server/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watchAll",
    "build": "webpack -w",
    "start": "nodemon server/index.js",
    "pre-commit": "lint-staged",
    "create-hook": "husky add .husky/pre-commit 'npm run pre-commit && git add .'",
    "postinstall": "husky install && npm run create-hook"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {
    "@testing-library/react": "^12.1.0",
    "axios": "^0.21.4",
    "bootstrap": "^5.1.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-multi-carousel": "^2.6.5",
    "styled-components": "^5.3.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "file-loader": "^6.2.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
